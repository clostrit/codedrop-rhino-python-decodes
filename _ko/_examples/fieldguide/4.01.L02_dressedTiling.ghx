                    <item name="CodeInput" type_name="gh_string" type_code="10"># Create three segments for every node
def node(seed,flags):
    rgons = []
    # Create an RGon based on some flags
    for n in flags:
        rgons.append(RGon.from_edge(seed,n))
        seed = rgons[-1].edges[0]
    
    # Create a Segment half the length of every edge of the RGon
    segs = [Segment(rgon.pts[0],Vec(rgon.pts[0],rgon.pts[1])/2.0) for rgon in rgons]
    return segs

# Starting Segment for the tiling
seed = Segment(Point(1,1),Vec(0,1))
# Truncated square tiling
flags = (4,8,8) 

# Number of nodes per cluster
nodes_per_clst = 4
clst_per_row = u_cnt

sqrt2 = 2**0.5 # the width of an octogon is 1+sqrt(2)*side_length

for n in range(u_cnt*v_cnt*nodes_per_clst):
    if n&gt;0 and n%nodes_per_clst==0:
        # translations between u-clusters
        seed._pt += Vec(1+sqrt2*seed.length+seed.length,0)
    if n&gt;0 and n%(nodes_per_clst*clst_per_row)==0:
        # translations between v-clusters
        dy = 1+sqrt2*seed.length+seed.length
        #alternate odd and even rows
        if n%(nodes_per_clst*clst_per_row*2)==0 : dx = -dy*(clst_per_row+0.5)
        else : dx = -dy*(clst_per_row-0.5)
        seed += Vec(dx,dy/2.0)
    if n&gt;0:
        # translations within a cluster
        seed = Segment(seed.ept,seed._vec.cross(Vec(0,0,1)))
    
    cur_node = node(seed,flags)
    a.put(cur_node)
